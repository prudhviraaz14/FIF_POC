<action-mapping>
    <action-name>createOnlineContract</action-name>
    <message-creation>
        <creator-type>xslt</creator-type>
        <creator-params>
            <creator-param>
                <param-name>filename</param-name>
                <param-value>KBACreateOnlineContract.xsl</param-value>
            </creator-param>
        </creator-params>
    </message-creation>
    <message-parameters>
        <message-param>
            <param-name>transactionID</param-name>
            <param-mandatory>true</param-mandatory>
        </message-param>
        <message-param>
            <param-name>OVERRIDE_SYSTEM_DATE</param-name>
            <param-mandatory>false</param-mandatory>
        </message-param>
        <message-param>
            <param-name>requestListId</param-name>
            <param-mandatory>false</param-mandatory>
        </message-param>        
        <message-param>
            <param-name>clientName</param-name>
            <param-mandatory>false</param-mandatory>
            <param-default-value>CODB</param-default-value>
        </message-param>
        <message-param>
            <param-name>serviceSubscriptionId</param-name>
            <param-mandatory>false</param-mandatory>
        </message-param>
        <message-param>
            <param-name>productCode</param-name>
            <param-mandatory>false</param-mandatory>
            <param-default-refdata-value>
                <group-code>KBA_CR_ONL</group-code>
                <item-identifier>PRODUCT_CODE</item-identifier>
            </param-default-refdata-value>
        </message-param>
        <message-param>
            <param-name>productVersionCode</param-name>
            <param-mandatory>false</param-mandatory>
            <param-default-refdata-value>
                <group-code>KBA_CR_ONL</group-code>
                <item-identifier>PRODUCT_VERSION_CODE</item-identifier>
            </param-default-refdata-value>
        </message-param>
        <message-param>
            <param-name>tariffOnline</param-name>
            <param-mandatory>true</param-mandatory>
        </message-param>
        <message-param>
            <param-name>salesOrganisationNumber</param-name>
            <param-mandatory>true</param-mandatory>
        </message-param>
        <message-param>
            <param-name>skeletonContractNumber</param-name>
            <param-mandatory>false</param-mandatory>
        </message-param>
        <message-param>
            <param-name>dueDate</param-name>
            <param-mandatory>true</param-mandatory>
        </message-param>
        <message-param>
            <param-name>minPeriodDurationValue</param-name>
            <param-mandatory>false</param-mandatory>
            <param-default-value>12</param-default-value>
        </message-param>
        <message-param>
            <param-name>minPeriodDurationUnit</param-name>
            <param-mandatory>false</param-mandatory>
            <param-default-value>MONTH</param-default-value>
        </message-param>
        <message-param>
            <param-name>OMTSOrderID</param-name>
            <param-mandatory>false</param-mandatory>
        </message-param>
        <message-param>
            <param-name>technologyChangeIndicator</param-name>
            <param-mandatory>false</param-mandatory>
            <param-default-value>N</param-default-value>
        </message-param>        
        <message-param>
            <param-name>salesOrganisationNumberVF</param-name>
            <param-mandatory>false</param-mandatory>
        </message-param>         
        <message-param>
            <param-name>reasonRd</param-name>
            <param-mandatory>false</param-mandatory>
        </message-param>
        <message-param>
            <param-name>voiceFunctionID</param-name>
            <param-mandatory>false</param-mandatory>
        </message-param>
    </message-parameters>
    <response-handling>
        <return-warnings>false</return-warnings>
        <output-parameters>
            <output-param>
                <response-command-id>getPTN</response-command-id>
                <response-param-name>output_string</response-param-name>
                <output-param-name>internetProviderTrackingNumber</output-param-name>
            </output-param>
        </output-parameters>
    </response-handling>
</action-mapping>
