{file:templates/OrderHeader.xml_part}
{file:templates/CustomerData.xml_part}
	<relocation orderPositionNumber="1">
		{file:templates/OrderPositionHeader.xml_part}
		<accesses>
			{file:templates/NGNAccessNewOld.xml_part}
		</accesses>
		<functions>
			{file:templates/InternetOldNew.xml_part}
			<voice ID="voice" refAccessID="access-old">
				<processingType>CHANGE</processingType>
				<ccbId type="S">
				    <existing>{voiceSSID}</existing>
				    <configured>{voiceSSIDNew}</configured>
				</ccbId>
				<providerTrackingNumber type="default">
					<configured>ptnDefaultVoice</configured>
				</providerTrackingNumber>
				<providerTrackingNumber type="change">
					<configured>ptnChangeVoice</configured>
				</providerTrackingNumber>
				<providerTrackingNumber type="target">
					<configured>ptnTargetVoice</configured>
				</providerTrackingNumber>
				<voiceBasisConfiguration>
					<productCode>
						<existing>{productCodeVoice}</existing>
					</productCode>
					<pricingStructureCode>
						<existing>{pricingStructureCodeVoice}</existing>
					</pricingStructureCode>
					<mainAccessServiceCode>
						<existing>{mainAccessServiceCodeVoice}</existing>
					</mainAccessServiceCode>
					<visTrackingPosition/>
					<primaryCustSignName>
						<existing>COM {scenarioName}</existing>
					</primaryCustSignName>
					<primaryCustSignDate>
						<existing>{todayTruncated}</existing>
					</primaryCustSignDate>
					<minimumDurationPeriod>
						<value>
							<existing>24</existing>
						</value>
						<unit>
							<existing>MONTH</existing>
						</unit>
					</minimumDurationPeriod>
					<automaticMailingIndicator>
						<existing>true</existing>
					</automaticMailingIndicator>
					<projectOrderIndicator>
						<existing>false</existing>
					</projectOrderIndicator>
					<activationType>
						<existing>Neuanschluss</existing>
					</activationType>
					<callForwarding>
						<targetPhoneNumber>
							<existing>
								<countryCode>49</countryCode>
								<localAreaCode>221</localAreaCode>
								<pilotNumber>{accessNumber1}</pilotNumber>
							</existing>
						</targetPhoneNumber>
					</callForwarding>					
					<tariffOptionsList>
					    <existing>
					        <tariffOption>
					            <serviceCode>V0306</serviceCode>
					        </tariffOption>
					    </existing>
					</tariffOptionsList>
					<portingAccessNumbers>
						<portingMode>
							<existing>None</existing>
						</portingMode>
						<fixedOrderDateIndicator>
							<existing>false</existing>
						</fixedOrderDateIndicator>
						<numberOfNewAccessNumbers>
							<existing>3</existing>
						</numberOfNewAccessNumbers>
						<oldLocalExchangeCarrier>
							<existing>DTAG</existing>
						</oldLocalExchangeCarrier>
						<maskingDigits>
							<existing>0</existing>
						</maskingDigits>
						<storageMaskingDigits>
							<existing>-1</existing>
						</storageMaskingDigits>
						<accessNumber1>
							<existing>
								<countryCode>49</countryCode>
								<localAreaCode>211</localAreaCode>
								<pilotNumber>{accessNumber1}</pilotNumber>
							</existing>
						</accessNumber1>
						<ownerAccessNumber1>
							<existing>D001</existing>
						</ownerAccessNumber1>
						<voipAccount1>
							<existing>492111700123@vodafone.de</existing>
						</voipAccount1>
						<lineOwner1>
							<existing>
								<firstName>COM</firstName>
								<name>{scenarioName}</name>
							</existing>
						</lineOwner1>
						<lineOwnerAddress>
							<existing>
								{file:templates/Address1.xml_part}
							</existing>
						</lineOwnerAddress>
					</portingAccessNumbers>
					<remarks>
						<existing>GK-Vertrieb B3, IMP, Betreute Schaltung nein, Privatkunde / Telefonbucheintrag / </existing>
					</remarks>
				</voiceBasisConfiguration>
				<processingStatus>{voiceProcessingStatus}</processingStatus>
				<completionStatusOldCcbId>{voiceCompletionStatusOld}</completionStatusOldCcbId>
				<previousCompletionStatusOldCcbId>{voicePreviousCompletionStatusOld}</previousCompletionStatusOldCcbId>
				<completionStatusNewCcbId>{voiceCompletionStatusNew}</completionStatusNewCcbId>
				<previousCompletionStatusNewCcbId>{voicePreviousCompletionStatusNew}</previousCompletionStatusNewCcbId>
			</voice>
			<mobileUsage ID="mobileUsage">
				<processingType>IGNORE</processingType>
				<ccbId type="S">
				    <existing>{mobileUsageSSID}</existing>
				    <configured>{mobileUsageSSIDNew}</configured>
				</ccbId>
				<providerTrackingNumber type="default">
					<configured>ptnDefaultMU</configured>
				</providerTrackingNumber>
				<providerTrackingNumber type="change">
					<configured>ptnChangeMU</configured>
				</providerTrackingNumber>
				<providerTrackingNumber type="target">
					<configured>ptnTargetMU</configured>
				</providerTrackingNumber>
				<refFunctionID>
					<existing>internet</existing>
				</refFunctionID>
				<mobileUsageConfiguration>
					<accessNumber>
						<existing>
							<countryCode>49</countryCode>
							<localAreaCode>172</localAreaCode>
							<pilotNumber>{accessNumber1}</pilotNumber>						
						</existing>
					</accessNumber>
					<billingAccountNumber>
						<existing>{accessNumber1}</existing>					
					</billingAccountNumber>
					<simSerialNumber>
						<existing>SIM{accessNumber1}</existing>					
					</simSerialNumber>					
				</mobileUsageConfiguration>
				<processingStatus>{mobileUsageProcessingStatus}</processingStatus>
				<completionStatusOldCcbId>{mobileUsageCompletionStatusOld}</completionStatusOldCcbId>
				<previousCompletionStatusOldCcbId>{mobileUsagePreviousCompletionStatusOld}</previousCompletionStatusOldCcbId>
			</mobileUsage>
		</functions>
	</relocation>
</order>
