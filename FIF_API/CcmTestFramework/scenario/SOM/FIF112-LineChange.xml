					    <existing>
					        <tariffOption>
					            <serviceCode>V1200</serviceCode>
					            <minimumDurationPeriod>
					            	<unit>MONTH</unit>
					            	<value>12</value>
					            	<startDate>{startDateTO}</startDate>					            	
					            </minimumDurationPeriod>
					        </tariffOption>
					        <tariffOption>
					            <serviceCode>V1205</serviceCode>
					            <minimumDurationPeriod>
