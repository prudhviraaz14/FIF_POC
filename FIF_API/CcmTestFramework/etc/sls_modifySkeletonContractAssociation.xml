<action-mapping>
	<action-name>modifySkeletonContractAssociation</action-name>
	<message-creation>
		<creator-type>xslt</creator-type>
		<creator-params>
			<creator-param>
				<param-name>filename</param-name>
				<param-value>SLSModifySkeletonContractAssociation.xsl</param-value>
			</creator-param>
		</creator-params>
	</message-creation>
	<message-parameters>
		<message-param>
			<param-name>transactionID</param-name>
			<param-mandatory>true</param-mandatory>
		</message-param>
		<message-param>
			<param-name>OVERRIDE_SYSTEM_DATE</param-name>
			<param-mandatory>false</param-mandatory>
		</message-param>
		<message-param>
			<param-name>clientName</param-name>
			<param-mandatory>false</param-mandatory>
			<param-default-value>SLS</param-default-value>
		</message-param>

		<message-param>
			<param-name>skeletonContractNumber</param-name>
			<param-mandatory>true</param-mandatory>
			<param-validation-method>validateRegularExpression</param-validation-method>
		</message-param>

		<message-param>
			<param-name>contractNumber</param-name>
			<param-mandatory>false</param-mandatory>
			<param-validation-method>validateRegularExpression</param-validation-method>
		</message-param>
		<message-param>
			<param-name>contractType</param-name>
			<param-mandatory>false</param-mandatory>
			<param-validation-method>validateRegularExpression</param-validation-method>
		</message-param>
		<message-param>
			<param-name>action</param-name>
			<param-mandatory>false</param-mandatory>
			<param-validation-method>validateRegularExpression</param-validation-method>
		</message-param>
		
		<message-param-list>
			<param-list-name>contractList</param-list-name>
			<param-list-mandatory>false</param-list-mandatory>
			<message-param>
				<param-name>contractNumber</param-name>
				<param-mandatory>true</param-mandatory>
				<param-validation-method>validateRegularExpression</param-validation-method>
			</message-param>
			<message-param>
				<param-name>contractType</param-name>
				<param-mandatory>true</param-mandatory>
				<param-validation-method>validateRegularExpression</param-validation-method>
			</message-param>
			<message-param>
				<param-name>action</param-name>
				<param-mandatory>true</param-mandatory>
				<param-validation-method>validateRegularExpression</param-validation-method>
			</message-param>
		</message-param-list>
		
		<!-- parameters for signatures -->
		<message-param>
			<param-name>boardSignName</param-name>
			<param-mandatory>false</param-mandatory>
			<param-default-value>Vodafone</param-default-value>
		</message-param>
		<message-param>
			<param-name>boardSignDate</param-name>
			<param-mandatory>false</param-mandatory>
			<param-validation-method>validateCCBDate</param-validation-method>
		</message-param>
		<message-param>
			<param-name>primaryCustSignName</param-name>
			<param-mandatory>false</param-mandatory>
			<param-default-value>Kunde</param-default-value>
		</message-param>
		<message-param>
			<param-name>primaryCustSignDate</param-name>
			<param-mandatory>false</param-mandatory>
			<param-validation-method>validateCCBDate</param-validation-method>
		</message-param>
		<message-param>
			<param-name>secondaryCustSignName</param-name>
			<param-mandatory>false</param-mandatory>
		</message-param>
		<message-param>
			<param-name>secondaryCustSignDate</param-name>
			<param-mandatory>false</param-mandatory>
			<param-validation-method>validateCCBDate</param-validation-method>
		</message-param>
		
		<message-param>
			<param-name>salesOrganisationNumber</param-name>
			<param-mandatory>false</param-mandatory>
		</message-param>
		
		<!-- parameters for creating contacts -->
		<message-param>
			<param-name>createContact</param-name>
			<param-mandatory>false</param-mandatory>
			<param-default-value>Y</param-default-value>
			<param-validation-method>validateCCMIndicator</param-validation-method>
		</message-param>
		<message-param>
			<param-name>contactTypeRd</param-name>
			<param-mandatory>false</param-mandatory>
		</message-param>
		<message-param>
			<param-name>shortDescription</param-name>
			<param-mandatory>false</param-mandatory>
		</message-param>
		<message-param>
			<param-name>longDescriptionText</param-name>
			<param-mandatory>false</param-mandatory>
		</message-param>
		
	</message-parameters>
	<response-handling>
		<return-warnings>false</return-warnings>
		<output-parameters/>
	</response-handling>
</action-mapping>
